<% for (let i = 0; i < posts.length && (locals.noLimit || i < config.recentPostsCutoff); i++){ %>
	<% let post = posts[i]; %>

	<article>
		<h2 class="inline"><a href="/posts/<%= post.slug %>"><%= post.title %></a></h2> <em class="publish-date"><%= post.date %></em>
		<!-- <p class="summary"></p> -->
	</article>
<% } %>
