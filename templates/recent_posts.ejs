<div class="clearfix">
	<a title="RSS feed" href="/feed.rss" class="icon right no-underline">
		<svg viewBox="0 0 24 24" role="presentation"><path d="M5,3H19A2,2 0 0,1 21,5V19A2,2 0 0,1 19,21H5A2,2 0 0,1 3,19V5A2,2 0 0,1 5,3M7.5,15A1.5,1.5 0 0,0 6,16.5A1.5,1.5 0 0,0 7.5,18A1.5,1.5 0 0,0 9,16.5A1.5,1.5 0 0,0 7.5,15M6,10V12A6,6 0 0,1 12,18H14A8,8 0 0,0 6,10M6,6V8A10,10 0 0,1 16,18H18A12,12 0 0,0 6,6Z" style="fill: currentcolor;" /></svg>
	</a>
	<% for (let i = 0; i < posts.length && (locals.noLimit || i < config.recentPostsCutoff); i++){ %>
		<% let post = posts[i]; %>
		<article>
			<h2 class="inline"><a href="/posts/<%= post.slug %>"><%= post.title %></a></h2> <em class="publish-date"><%= post.date %></em>
			<!-- <p class="summary"></p> -->
		</article>
	<% } %>
	<% if (posts.length > config.recentPostsCutoff){ %>
		<a href="/archive">More...</a>
	<% } %>
</div>

